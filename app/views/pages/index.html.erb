<!-- Carousel
================================================== -->
<div id="myCarousel" class="carousel slide">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="item active">
      <img src="http://mchristopherandcompany.com/uploads/images/HeaderImages/model-homes-header.jpg" class="img-responsive">
      <div class="container">
        <div class="carousel-caption">
          <h1>Brand Yourself Using Mobile Ads</h1>
          <p><button class="video" style="padding:20px;">Open popup</button>
        </p></div>
      </div>
    </div>
    <div class="item">
      <img src="http://www.themichaelgroup.com/images/roomwithview.jpg" class="img-responsive">
      <div class="container">
        <div class="carousel-caption">
          <h1>Stay In front of your prospects.</h1>
          <p><a class="btn btn-large btn-primary" href="#">Learn more</a></p>
        </div>
      </div>
    </div>
    <div class="item">
      <img src="http://www.robertluckegroup.com/images/PhotoGallery/Robert%20Lucke%20Homes,%20exterior,%20elevation,%20ranch,%20cincinnati,%20ohio,%20luxury%20homes,%20custom%20homes.jpg" class="img-responsive">
      <div class="container">
        <div class="carousel-caption">
          <h1>Affordable and Easy</h1>
          <p>Easy, walk through setting up a campaign and pay!</p>
          <p><a class="btn btn-large btn-primary" href="#">Browse gallery</a></p>
        </div>
      </div>
    </div>
  </div>
  <!-- Controls -->
  <a class="left carousel-control" href="#myCarousel" data-slide="prev">
    <span class="icon-prev"></span>
  </a>
  <a class="right carousel-control" href="#myCarousel" data-slide="next">
    <span class="icon-next"></span>
  </a>  
</div>
<!-- /.carousel -->


<!-- Marketing messaging and featurettes
================================================== -->
<!-- Wrap the rest of the page in another container to center all the content. -->

<div class="container-fluid marketing">

  <!-- Three columns of text below the carousel -->
  <div class="row">
  <div class="container">
    <div class="col-md-4 text-center">
      <img class="img-circle" src="http://inospy.com/wp-content/uploads/2014/09/smartphone-and-tablet.png" height="140" width="140">
      <h2>Mobile and Tablets</h2>
      <p>Be seen on the devices your prospects are on, theirs.</p>
      <a class="btn btn-default" href="#">Create ads »</a>
    </div>
    <div class="col-md-4 text-center">
      <img class="img-circle" src="http://www.ipsumtm.com/wp-content/uploads/2013/11/Geo-Targeting.jpg" height="140" width="140">
      <h2>Target locally</h2>
      <p>Only local prospects will see your ads.</p>
      <p><a class="btn btn-default" href="#">Create ads »</a></p>
    </div>
    <div class="col-md-4 text-center">
      <img class="img-circle" src="http://forums.macrumors.com/picture.php?albumid=857&pictureid=4367" height="140" width="140">
      <h2>Easy Setup</h2>
      <p>Walk through a setup and be ready in minutes.</p>
      <p><a class="btn btn-default" href="#">Create ads »</a></p>
    </div>
    </div>
  </div><!-- /.row -->

  <hr>
  <%= render 'parallax' %>
  <hr>

  <!-- START THE FEATURETTES -->

  <div class="featurette">
    <img class="featurette-image img-circle pull-left" src="http://placehold.it/512">
    <h2 class="featurette-heading">Farm Your Neighborhood. <span class="text-muted">Digitally.</span></h2>
    <p class="lead">Make sure buyers and sellers know who you are when it comes time for them to pick a real estate agent. Stay in front of of your prospects, constantly. Be there when they are ready to decide.</p>
  </div>

  <div class="featurette feat-blue">
    <img class="featurette-image img-circle pull-right" src="http://placehold.it/512">
    <h2 class="featurette-heading pull-right">Great Prices. <span class="text-muted">Unbelievable value.</span></h2>
    <p class="lead pull-right">Farm your desired zip codes by putting your ads constantly in front of people, year round. </p>
  </div>


  <!-- /END THE FEATURETTES -->
  <%= render 'pricing' %>

</div><!-- /.container -->

<!-- FOOTER -->
  <footer>
    <p class="pull-right"><a href="#">Back to top</a></p>
  </footer>

</div><!-- /.container -->
